{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es6",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "baseUrl": "./",
    "skipLibCheck": true,
    "paths": {
      "@environments/*": [
        "./src/environments/*"
      ],
      "@features/*": [
        "./src/app/features/*"
      ],
      "@shared/*": [
        "./src/app/shared/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@sass/*": [
        "./src/sass/*"
      ],
      "@core/*": [
        "./src/app/core/*"
      ]
    }
  },
  
}
